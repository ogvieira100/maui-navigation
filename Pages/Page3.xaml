<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="maui_navigation.Pages.Page3"
             Title="Page3">
    <VerticalStackLayout>
        <Button
            HeightRequest="50"
            Text="Go to Root Page"
            x:Name="btnGotoRootPage"
            Clicked="btnGotoRootPage_Clicked"
            />
        <Button
            HeightRequest="50"
            Text="Go to Modal Page"
            x:Name="btnGotoModalPage"
            Clicked="btnGotoModalPage_Clicked"
            />
    </VerticalStackLayout>
</ContentPage>